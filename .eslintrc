{
  "extends": ["eslint:recommended", "prettier", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "root": true,
  "rules": {
    "sort-imports": [
      "error",
      {
        "ignoreCase": true,
        "allowSeparatedGroups": true
      }
    ]
  }
}
